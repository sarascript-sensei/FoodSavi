<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.foodsaving">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MapActivityRestaurant"></activity>
        <activity android:name=".RestaurantRegister" />
        <activity android:name=".WelcomeActivity" />
        <activity android:name=".ui.login.WelcomeActivity" />
        <activity android:name=".CustomerSignUp" />
        <activity android:name=".ui.login.CustomerSignUp" />
        <activity
            android:name=".ui.login.CustomerLoginActivity"
            android:label="@string/title_activity_customer_login" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>